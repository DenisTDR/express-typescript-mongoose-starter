{
    "name": "express-typescript-mongoose-starter",
    "version": "1.0.0",
    "description": "A starter for Node JS, Express, Typescript, Mongoose aplication",
    "main": "server.js",
    "scripts": {
        "serve": "npm run build-ts & npm run start",
        "build-ts": "tsc -p src",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --type-check ./src/server.ts"
    },
    "keywords": [
        "Node",
        "JS",
        "Express",
        "Mongo",
        "Mongoose",
        "Typescript"
    ],
    "author": "Matheus Davidson - MTDA",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "dotenv": "^5.0.1",
        "ejs": "^2.6.1",
        "express": "^4.16.3",
        "express-validator": "^5.2.0",
        "jwt-simple": "^0.5.1",
        "moment": "^2.22.2",
        "mongoose": "^5.1.4",
        "morgan": "^1.9.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.1",
        "@types/express": "^4.16.0",
        "@types/lodash": "^4.14.109",
        "@types/mongodb": "^3.0.19",
        "@types/mongoose": "^5.0.15",
        "@types/node": "^10.3.1",
        "glob": "^7.1.2",
        "lodash": "^4.17.10",
        "ts-node-dev": "^1.0.0-pre.26",
        "typescript": "^2.9.1"
    }
}
